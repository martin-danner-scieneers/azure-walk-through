{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-rundstedt"
		},
		"AzureBlobStorageRundstedt_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageRundstedt'"
		},
		"RestService1_clientSecret": {
			"type": "secureString",
			"metadata": "Secure string for 'clientSecret' of 'RestService1'"
		},
		"AzureMLRundstedt_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "3aef07e0-41f3-4130-bc27-9d3652939fea"
		},
		"AzureMLRundstedt_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "walk-through-azureml-rundstedt"
		},
		"AzureMLRundstedt_properties_typeProperties_mlWorkspaceName": {
			"type": "string",
			"defaultValue": "azml-rundstedt"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://flaskjobrecommendationapp.azurewebsites.net"
		},
		"RestService1_properties_typeProperties_clientId": {
			"type": "string",
			"defaultValue": "39bec76c-28f5-4844-aee6-08cc07c15cb8"
		},
		"trigger_test_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/3aef07e0-41f3-4130-bc27-9d3652939fea/resourceGroups/walk-through-azureml-rundstedt/providers/Microsoft.Storage/storageAccounts/dlrundstedt"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-rundstedt_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageRundstedt_connectionString": {
						"value": "[parameters('AzureBlobStorageRundstedt_connectionString')]"
					},
					"RestService1_clientSecret": {
						"value": "[parameters('RestService1_clientSecret')]"
					},
					"AzureMLRundstedt_properties_typeProperties_subscriptionId": {
						"value": "[parameters('AzureMLRundstedt_properties_typeProperties_subscriptionId')]"
					},
					"AzureMLRundstedt_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('AzureMLRundstedt_properties_typeProperties_resourceGroupName')]"
					},
					"AzureMLRundstedt_properties_typeProperties_mlWorkspaceName": {
						"value": "[parameters('AzureMLRundstedt_properties_typeProperties_mlWorkspaceName')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_clientId": {
						"value": "[parameters('RestService1_properties_typeProperties_clientId')]"
					},
					"trigger_test_properties_typeProperties_scope": {
						"value": "[parameters('trigger_test_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}